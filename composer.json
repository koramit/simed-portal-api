{
    "name": "koramit/simed-portal-api",
    "description": "Laravel package for SiMED Portal API",
    "type": "library",
    "require": {
        "php": "^8.3.0",
        "illuminate/support": "^11.43|^12.0",
        "illuminate/http": "^12.39"
    },
    "require-dev": {
        "laravel/pint": "^1.24"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Koramit\\SiMEDPortalAPI\\": "src/"
        }
    },
    "authors": [
        {
            "name": "koramit",
            "email": "koramit.p@gmail.com"
        }
    ],
    "extra": {
      "laravel": {
        "providers": [
          "Koramit\\SiMEDPortalAPI\\PortalAPIServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
      "lint": "pint"
    }
}
